{
  "name": "rwanda-info-view-api",
  "version": "1.0.0",
  "description": "Rwanda Info View API",
  "main": "app.ts",
  "scripts": {
    "test": "mocha",
    "dev": "nodemon",
    "build": "tsc && tsc-alias && npm run copy-env",
    "migrate": "sequelize-cli db:migrate",
    "migrate:undo": "sequelize-cli db:migrate:undo",
    "migrate:undo:all": "sequelize-cli db:migrate:undo:all",
    "seed": "sequelize-cli db:seed:all",
    "seed:undo": "sequelize-cli db:seed:undo:all",
    "start": "node dist/app.js",
    "copy-env": "cp src/configs/env-config.js dist/src/configs/env-config.js",
    "deploy": "rm -rf dist && tar zxvf dist.tar.gz",
    "compress": "tar zcvf dist.tar.gz dist/",
    "build-prod": "npm run build && npm run compress"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/AJAkimana/stock-api.git"
  },
  "keywords": [
    "stock",
    "commercial",
    "app",
    "Akimana",
    "Aja"
  ],
  "author": "AJAkimana",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AJAkimana/stock-api/issues"
  },
  "homepage": "https://github.com/AJAkimana/stock-api#readme",
  "dependencies": {
    "axios": "^1.10.0",
    "bcryptjs": "^3.0.2",
    "compression": "^1.8.0",
    "connect-redis": "^9.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-session": "^1.18.1",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.16.0",
    "redis": "^5.6.0",
    "sequelize": "^6.37.7"
  },
  "devDependencies": {
    "@types/chai": "^5.2.3",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/express-session": "^1.18.2",
    "@types/lodash": "^4.17.19",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.15.30",
    "@types/passport": "^1.0.17",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.33.1",
    "@typescript-eslint/parser": "^8.33.1",
    "chai": "^6.2.2",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.1",
    "mocha": "^11.7.5",
    "nodemon": "^3.1.10",
    "nyc": "^17.1.0",
    "prettier": "^3.5.3",
    "sequelize-cli": "^6.6.3",
    "supertest": "^7.2.2",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.21.0",
    "typescript": "^5.8.3"
  }
}
